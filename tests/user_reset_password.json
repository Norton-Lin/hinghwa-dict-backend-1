                    "            if (Array.isArray(value)) {",
                    "              value = Number(undefined) >= 0 ? value[undefined] : value[value.length + Number(undefined)];",
                    "            } else {",
                    "              value = undefined;",
                    "            }",
                    "          }",
                    "          ",
                    "        ",
                    "        switch (typeof value) {",
                    "          case 'object':",
                    "            value = JSON.stringify(value);",
                    "            break;",
                    "          default:",
                    "            value = String(value);",
                    "            break;",
                    "        }",
                    "      ",
                    "        pm.environment.set(`token`, value);console.log('___label_placeholder__processor___', '已设置环境变量【token】，值为 【' + value + '】')",
