                  "exec": [
                    "",
                    "        if (JSON.setEnableBigInt) {",
                    "          JSON.setEnableBigInt(undefined);",
                    "        }",
                    "        ",
                    "      try{",
                    "        ",
                    "          const expression = pm.variables.replaceIn(`$.token`);",
                    "          const JSONPath = require('jsonpath-plus').JSONPath;",
                    "          const jsonData = pm.response.json();",
                    "          let value = JSONPath({",
                    "            json: jsonData,",
                    "            path: expression,",
                    "            wrap: false",
                    "          });",
                    "",
                    "",
                    "          if (false && undefined !== undefined) {",
                    "            if (Array.isArray(value)) {",
                    "              value = Number(undefined) >= 0 ? value[undefined] : value[value.length + Number(undefined)];",
                    "            } else {",
                    "              value = undefined;",
                    "            }",
                    "          }",
                    "          ",
                    "        ",
                    "        switch (typeof value) {",
                    "          case 'object':",
                    "            value = JSON.stringify(value);",
                    "            break;",
                    "          default:",
                    "            value = String(value);",
                    "            break;",
                    "        }",
                    "      ",
                    "        pm.environment.set(`token`, value);console.log('___label_placeholder__processor___', '已设置环境变量【token】，值为 【' + value + '】')",
                    "      } catch(e) {",
                    "        e.message = '提取变量【token】出错: ' + e.message;",
                    "        throw e;",
                    "      }",
                    "  ",
                    "      "
                  ]
                }
              }
